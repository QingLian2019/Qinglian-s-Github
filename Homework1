title_list = titles.split()

len(title_list)

from IPython.core.display import display,HTML
import jieba

def word_match(word,title):
    result = (word in title)
    return result

keyword = '生'

title_list = set(title_list) #去重
keyword_ = ' '.join(jieba.cut(keyword)) #为了去除空格的影响

for title in title_list:
    ifmatch = True
    for word in keyword_.split():         #keyword.split():自动分词效果split不受空格影响，因为它以特殊字符分割搜索词，但是没有识别词语的作用
        title = title.replace(word,'<span style="color:red">{}</span>'.format(word))
        if not word_match(word,title):
            ifmatch = False
        if ifmatch:
           # print(title.replace(keyword,'*{}*'.format(keyword)))
           #display(title.replace(keyword,'*{}*'.format(keyword)))  #与上面的同一效果
           # display(HTML(title.replace(keyword,'<span style="color:red">{}</span>'.format(keyword))))  
          #display(HTML(title.replace(keyword, '<p style="color:red">{}</p>'.format(keyword)))) #字体标红作用，但是会自动换行
            display(HTML(title))

        
